          "editorMode": "code",
          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": "SELECT \r\n  DATE_TRUNC('second', t.\"timestamp\") AS \"time\",\r\n  cast(temp.value as integer) AS \"temperature\",\r\n  cast(press.value as integer) AS \"pressure\",\r\n  cast(humid.value as integer) AS \"humidity\"\r\nFROM \r\n  (SELECT DISTINCT DATE_TRUNC('second', \"timestamp\") AS \"timestamp\" FROM telemetry WHERE device_id = ${device_ID}) t\r\nJOIN \r\n  (SELECT DATE_TRUNC('second', \"timestamp\") AS \"timestamp\", value FROM telemetry WHERE device_id = ${device_ID} AND name = 'temperature') temp\r\n  ON t.\"timestamp\" = temp.\"timestamp\"\r\nJOIN \r\n  (SELECT DATE_TRUNC('second', \"timestamp\") AS \"timestamp\", value FROM telemetry WHERE device_id = ${device_ID} AND name = 'pressure') press\r\n  ON t.\"timestamp\" = press.\"timestamp\"\r\nJOIN \r\n  (SELECT DATE_TRUNC('second', \"timestamp\") AS \"timestamp\", value FROM telemetry WHERE device_id = ${device_ID} AND name = 'humidity') humid\r\n  ON t.\"timestamp\" = humid.\"timestamp\"\r\nWHERE $__timeFilter(t.\"timestamp\")",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "alias": "\"time\"",
                "parameters": [
                  {
                    "name": "\"timestamp\"",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              },
              {
                "alias": "\"value\"",
                "parameters": [
                  {
                    "name": "value",
